#sign_up.form
  %h2 Sign up

  - semantic_form_for resource_name, resource, :url => registration_path(resource_name) do |f|
    = f.error_messages
    - f.inputs do
      = f.input :email
      = f.input :password
      = f.input :password_confirmation
    - f.buttons do
      %button{:type => 'submit'} Sign in
      = f.submit "Sign up"

